function diff(arr1,arr2) {
  var newArr = [];

  function check(arr,othrArr) {
    arr.forEach(function(element) {
      if (othrArr.indexOf(element) == -1) {
       newArr.push(element);
       console.log(newArr);
      }
    });
  }

  check(arr1,arr2);
  check(arr2,arr1);

  return newArr;
}

diff([1,2,3,5],[1,2,3,4,5]);
